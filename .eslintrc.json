{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "env":  {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "space-before-function-paren": 0,
    "no-underscore-dangle": 0
  }
}
